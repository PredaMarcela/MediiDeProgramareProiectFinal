@page
@model MediiDeProgramareProiectFinal.Pages.Studenti.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Creare</h1>

<h4>Student</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Student.Nume" class="control-label"></label>
                <input asp-for="Student.Nume" class="form-control" />
                <span asp-validation-for="Student.Nume" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Student.An_studiu" class="control-label"></label>
                <input asp-for="Student.An_studiu" class="form-control" />
                <span asp-validation-for="Student.An_studiu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Creare" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Inapoi la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
